<menu id="settings_menu" label="Settings">
  <item label="Openbox Config">
    <action name="Execute">
      <command>obconf</command>
    </action></item>
 <item label="Openbox Appearance">
    <action name="Execute">
      <command>lxappearance</command>
    </action></item>
 <item label="Openbox Panel">
    <action name="Execute">
      <command>tint2conf</command>
    </action>
  </item>
  <item label="Openbox Autostart"><action name="Execute"><command><![CDATA[geany ~/.config/openbox/autostart]]></command></action></item>
  <item label="Openbox RC"><action name="Execute"><command><![CDATA[geany ~/.config/openbox/rc.xml]]></command></action></item>
  <item label="Openbox Menu"><action name="Execute"><command><![CDATA[geany ~/.config/openbox/menu.xml]]></command></action></item>
  <item label="Reconfigure Openbox"><action name="Execute"><command><![CDATA[openbox --reconfigure]]></command></action></item>

  <menu id="Advance Menu" label="Advance Settings">
    <item label="Conky RC"><action name="Execute"><command><![CDATA[geany ~/.conkyrc]]></command></action></item>
    <item label="Tint2 Panel"><action name="Execute"><command><![CDATA[geany ~/.config/tint2/tint2rc]]></command></action></item>
    <menu id="0.138062832660392" label="Obmenu-Generator">
      <item label="Menu Schema"><action name="Execute"><command><![CDATA[geany ~/.config/obmenu-generator/schema.pl]]></command></action></item>
      <item label="Menu Config"><action name="Execute"><command><![CDATA[geany ~/.config/obmenu-generator/config.pl]]></command></action></item>
      <separator/>
      <item label="Generate a static menu"><action name="Execute"><command><![CDATA[obmenu-generator -s -c]]></command></action></item>
      <item label="Generate a static menu with icons"><action name="Execute"><command><![CDATA[obmenu-generator -s -i -c]]></command></action></item>
      <separator/>
      <item label="Generate a dynamic menu"><action name="Execute"><command><![CDATA[obmenu-generator -p]]></command></action></item>
      <item label="Generate a dynamic menu with icons"><action name="Execute"><command><![CDATA[obmenu-generator -p -i]]></command></action></item>
      <separator/>
      <item label="Refresh cache"><action name="Execute"><command><![CDATA[obmenu-generator -d]]></command></action></item>
    </menu>
  </menu>

</menu>

<menu id="exit_menu" label="Exit">
 <item label="Log Out">
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
<item label="Reboot">
    <action name="Execute">
      <command>systemctl reboot</command>
    </action>
 </item>
<item label="Poweroff">
    <action name="Execute">
      <command>systemctl poweroff</command>
    </action>
 </item>

